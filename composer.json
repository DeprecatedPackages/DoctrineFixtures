{
	"name": "zenify/doctrine-fixtures",
	"description": "Doctrine Fixtures, Faker and Alice integration into Nette",
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "homepage": "http://tomasvotruba.cz" }
	],
	"require": {
		"php": ">=5.6",
		"kdyby/doctrine": "~2.3.0",
		"doctrine/data-fixtures": "~1.1.0",
		"fzaninotto/faker": "~1.4",
		"nelmio/alice": "~1.7",
		"nette/di": "~2.3.0",
		"nette/finder": "~2.3.0",
		"tracy/tracy": "~2.3.0"
	},
	"require-dev": {
		"nette/bootstrap": "~2.3.0"
	},
	"autoload": {
		"psr-4": {
			"Zenify\\DoctrineFixtures\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Zenify\\DoctrineFixtures\\Tests\\": "tests"
		}
	}
}
